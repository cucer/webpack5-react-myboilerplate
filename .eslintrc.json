{
  "env": {
    "browser": true,
    "es2021": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  // "extends": ["eslint:recommended"],
  "extends": [
    "prettier",
    "plugin:react/recommended",
    "plugin:react/jsx-runtime"
  ],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "warn",
    "react/prop-types": 1
  }
}
